      </sectionGroup>
    </sectionGroup>
  </configSections>
  <appSettings>
    <add key="FolderPath" value="~/TempImgMail"/>
  </appSettings>
  <connectionStrings>
    <add name="MarzzanConnectionString" connectionString="Data Source=srvtestbjrm01;Initial Catalog=SBDPLCD3;Persist Security Info=True;User ID=sa;Password=sa" providerName="System.Data.SqlClient"/>
    <add name="Marzzan_BejermanConnectionString" connectionString="Data Source=srvweblcda;Initial Catalog=MarzzanWebTest;Persist Security Info=True;User ID=sa;Password=sa123456" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <system.web>
    <identity impersonate="true"/>
    <globalization fileEncoding="iso-8859-1" requestEncoding="iso-8859-1" responseEncoding="iso-8859-1" culture="es-AR" uiCulture="es-AR"/>
    <customErrors mode="Off"/>
